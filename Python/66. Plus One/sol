class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        x = ""
        y = []
        for i in range(0, len(digits)):
            x += str(digits[i])
        ans = int(x) + 1
        for char in str(ans):
            y.append(int(char))
        return y
